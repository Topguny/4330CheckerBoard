<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox fx:id="vBox" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="830.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="board.FXMLDocumentController">
   <children>
      <MenuBar fx:id="menuBar" prefHeight="30.0" prefWidth="800.0">
         <menus>
            <Menu fx:id="gridMenu" mnemonicParsing="false" text="Grid">
               <items>
                  <MenuItem fx:id="size16" mnemonicParsing="false" onAction="#changeSize" text="16 x 16" />
                  <MenuItem fx:id="size10" mnemonicParsing="false" onAction="#changeSize" text="10 x 10" />
                  <MenuItem fx:id="size8" mnemonicParsing="false" onAction="#changeSize" text="8 x 8" />
                  <MenuItem fx:id="size3" mnemonicParsing="false" onAction="#changeSize" text="3 x 3" />
               </items>
            </Menu>
            <Menu id="colorMenu" fx:id="colorMenu" mnemonicParsing="false" text="Colors">
               <items>
                  <MenuItem fx:id="default" mnemonicParsing="false" onAction="#changeColor" text="Default" />
                  <MenuItem fx:id="blue" mnemonicParsing="false" onAction="#changeColor" text="Blue" />
               </items>
            </Menu>
         </menus>
      </MenuBar>
        <VBox fx:id="displayArea"> <AnchorPane fx:id="anchorPane" VBox.vgrow="ALWAYS" /> </VBox>
   </children>
</VBox>
